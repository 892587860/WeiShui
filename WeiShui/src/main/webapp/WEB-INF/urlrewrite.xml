<?xml version="1.0" encoding="UTF-8"?>
   <!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
	<rule>
		<from>/(.*).html</from>
		<to>/$1.jsp</to>
	</rule>
	<rule>
		<from>/login.html</from>
		<to>/login.html</to>
	</rule>
	<rule>
		<from>/(.*).jsp</from>
		<to>/error.jsp</to>
	</rule>
</urlrewrite>